{
  "env": {
    "browser": true,
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": "off",
    "new-cap": "off",
    "capitalized-comments": "off",
    "no-confusing-arrow": "off",
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
  },
}
